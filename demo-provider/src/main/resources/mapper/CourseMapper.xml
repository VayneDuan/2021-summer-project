<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.CourseMapper">
    <select id="findById" parameterType="java.lang.String" resultType="com.example.demo.pojo.GymClass">
        select *
        from gym_class
        where id = #{id}
    </select>
    <select id="findAll" parameterType="java.lang.String" resultType="com.example.demo.pojo.GymClass">
        select *
        from gym_class
    </select>
</mapper>